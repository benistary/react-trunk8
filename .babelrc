{
  "presets": [
    "react",
    "es2015"
  ],
  "passPerPreset": "true",
  "plugins": [
    "transform-class-properties",
    [ "transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": false
    }],
    ["tcomb", {
      "disable": {
         "production": true
       }
    }]
  ]
}
